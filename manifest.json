{
  "manifest_version": 2,
  "name": "Instapro",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action" : {},
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "icons/icon16x16.png",
    "48": "icons/icon48x48.png",
    "128": "icons/icon128x128.png"
    }
}